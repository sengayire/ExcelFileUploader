{
  "name": "Excel-file-uploader",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Ossix-Technologies/farmers-corner-backend.git",
  "author": "SENGAYIRE <psengayire@gmil.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ts-build": "rimraf build/ && tsc -p tsconfig.json",
    "build": "cross-env NODE_ENV=production babel src --extensions .js,.ts --out-dir build",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "start:dev": "nodemon",
    "dev": "yarn start:dev",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "db:create": "npm run sequelize db:create",
    "db:create:test": "cross-env NODE_ENV=test yarn db:create",
    "db:drop": "npm run sequelize db:drop",
    "db:drop:test": "cross-env NODE_ENV=test yarn db:drop",
    "db:migrate": "npm run sequelize db:migrate",
    "db:migrate:reset": "npm run sequelize db:migrate:undo:all",
    "db:seed": "npm run sequelize db:seed:all",
    "db:seed:dev": "cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "pretest": "yarn db:drop:test && yarn db:create:test && cross-env NODE_ENV=test yarn db:migrate && cross-env NODE_ENV=test yarn db:seed",
    "test": "jest --coverage --config=jest.config.js --passWithNoTests --detectOpenHandles --testTimeout=30000 --forceExit",
    "test:watch": "jest --coverage --config=jest.config.js --passWithNoTests --detectOpenHandles --testTimeout=30000 --forceExit --watch",
    "test:CI": "CI=true yarn test",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "celebrate": "12.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "helmet": "^4.4.1",
    "http-errors": "^1.8.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "request-ip": "^2.1.3",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/crypto-js": "^4.0.1",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-useragent": "^1.0.0",
    "@types/faker": "^5.5.7",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/request-ip": "^0.0.35",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  }
}
